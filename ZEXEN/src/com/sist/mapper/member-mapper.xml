<mapper namespace="com.sist.mapper.member-mapper">

<insert id="memberInsert" parameterType="com.sist.vo.MemberVO">
     INSERT INTO member_tb VALUES(
       #{member_id},
       #{pwd},
       #{name},
       #{email},
       #{tel},
       #{sex},
       #{birthday},
       #{addr1},
       #{addr2},
       'n',
       #{post}
     )
   </insert>

</mapper>